<div class="row">
  <div class="col-md-1"></div>
  <div class="col-md-10">
    <h1>Registered users</h1>
    <br>
    <br>
    <div class="row">
      <table class="table users-table">
        <tr class="title-tr">
          <td>Id</td>
          <td>Email</td>
          <td>Username</td>
          <td>Actions</td>
        </tr>
        <tr class="normal-tr" ng-repeat='user in users'>
          <td>{{user.id}}</td>
          <td>{{user.email}}</td>
          <td>{{user.username}}</td>
          <td class="actions">
            <a href="#!/users/show/{{user.id}}"><span class="glyphicon glyphicon-zoom-in" aria-hidden="true">Show </span></a>
            &nbsp;&nbsp;
            <a href="#!/users/edit/{{user.id}}"><span class="glyphicon glyphicon-pencil" aria-hidden="true">Edit </span></a>
          </td>
        </tr>
      </table>
    </div>
    <div class="row">
      <center>
        <a href="#!/users/new">
          <button class="btn btn-default normal-button" type="button" >New User</button>
        </a>
      </center>
    </div>
  </div>
  <div class="col-xs-1"></div>
</div>
<br><br><br>